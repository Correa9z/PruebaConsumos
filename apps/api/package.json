{
  "name": "api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "prisma generate && prisma migrate deploy && next dev --port 3001",
    "start": "next start --port 3001",
    "lint": "next lint",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf .next",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "dotenv": "^17.3.1",
    "next": "^15.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "swagger-ui-react": "^5.32.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.0",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.0.0",
    "jest": "^29.7.0",
    "prisma": "^5.22.0",
    "ts-jest": "^29.4.6",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  }
}
